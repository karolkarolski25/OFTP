      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>RegisterCodes.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\ServerRequestCodes.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>ServerRequestCodes.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\UserConnectionCodes.resx">

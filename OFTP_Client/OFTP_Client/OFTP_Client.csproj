    </Compile>
    <Compile Update="Resources\RegisterCodes.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>RegisterCodes.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\ServerRequestCodes.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>ServerRequestCodes.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\UserConnectionCodes.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>UserConnectionCodes.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resources\FileTransmissionCodes.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>FileTransmissionCodes.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\EmailCodes.resx">
      <Generator>ResXFileCodeGenerator</Generator>
